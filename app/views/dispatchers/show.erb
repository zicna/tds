
<h2><%= @team.name %> team</h2>
<h3>Dispacher <%= @dispatcher.name %> load board</h3>
<h4>All loads under <%= @dispatcher.name %>: </h4>
<table>
    <th>Pickup</th>
    <th>Delivery</th>
    <th>Miles</th>
    <th>Amount</th><br>

    <% @dispatcher.loads.each do |load| %>
        <tr>
            <td><%= load.pickup %></td>
            <td><%= load.delivery %></td>
            <td><%= load.miles %></td>
            <td><%= load.amount %></td>
        </tr>
    <% end %>
</table>

